package threading;
public class Sample1 {
	
	public void printing1()
	{
		for(int i=0;i<=5;i++)
		{
			System.out.println("Printing 1 to 5");
			System.out.println("i = "+i);
			try{
			Thread.sleep(2500);
			}
			catch(Exception e)
			{
				System.out.println("Handling Exception");
			}
		}
	}
	
	public void printing2()
	{
		for(int i=77;i<83;i++)
		{
			try{
				Thread.sleep(2500);
				}
				catch(Exception e)
				{
					System.out.println("Handling Exception");
				}
			System.out.println("Printing 77 to 82");
			System.out.println("i = "+i);
		}
	}
}
	

