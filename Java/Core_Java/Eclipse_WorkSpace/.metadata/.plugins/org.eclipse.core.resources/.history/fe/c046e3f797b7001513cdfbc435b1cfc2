package threading;

import java.lang.invoke.SwitchPoint;

public class Sample1 {
	
	int a,b,d=0,res=0;
	public Sample1(int a, int b) {
		this.a = a;
		this.b = b;
	}
	public static void main(String[] args) {
		switch(d)
		{
		case 1:
			res=a+b;
			System.out.println("Addition");
			break;
		default:
			System.out.println("Wrong Choice");
			}		
	}

